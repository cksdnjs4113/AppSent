<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chanwon.appsent" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Activity.LoginPage"
            android:label="@string/title_activity_home_page" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.HomePage"
            android:label="@string/title_activity_sub_page" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activity.LoginPage" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.OverviewSentiment"
            android:label="@string/title_activity_overview_sentiment" />
        <activity
            android:name=".Activity.TimeLinePopup"
            android:theme="@style/AppTheme.CustomTheme" />
        <activity
            android:name=".Activity.TimelinePopupEmo"
            android:theme="@style/AppTheme.CustomTheme"
            android:label="@string/title_activity_timeline_popup_emo" >
        </activity>
    </application>

</manifest>
